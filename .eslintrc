{
    "extends": ["eslint:recommended", "airbnb-base"],
    "parserOptions": { 
        "sourceType": "module",
        "ecmaVersion": 2018 
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "valid-jsdoc": [2, {
            "requireParamDescription": false,
            "requireReturnDescription": false,
            "requireReturn": false,
            "prefer": {
                "returns": "return"
            },
        }],
    }
}
